{
  "name": "photoalbum",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "container:deploy": "bash ./scripts/deploy.sh",
    "server:build": "yarn webpack -r dotenv/config --config ./webpack.config.js",
    "server:build:dev": "yarn webpack -r dotenv/config --config ./webpack.config.js",
    "server:start:": "node -r dotenv/config ./build/index.js",
    "server:start:dev": "yarn nodemon -r dotenv/config -L ./build/index.js",
    "start": "yarn run server:build & yarn run server:start",
    "start:dev": "yarn run server:build:dev & yarn run server:start:dev",
    "client:start:dev": "NODE_ENV=development rollup -c ./client/rollup.config.js -w"
  },
  "dependencies": {
    "@overnightjs/core": "^1.6.9",
    "@overnightjs/logger": "^1.1.9",
    "apollo-server-express": "^2.9.4",
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "http-status-codes": "^1.3.2",
    "mysql2": "^1.7.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.19.1",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "svelte": "^3.12.1",
    "type-graphql": "^0.17.5"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/graphql": "^14.5.0",
    "nodemon": "^1.19.2",
    "rollup": "^1.23.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.1.0",
    "ts-loader": "^6.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-node-externals": "^1.7.2"
  }
}
